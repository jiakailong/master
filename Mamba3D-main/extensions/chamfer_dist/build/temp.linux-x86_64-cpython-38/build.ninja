ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /media/jiang/jkl/env/Mamba3D/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -fwrapv -O2 -Wall -fPIC -O2 -isystem /media/jiang/jkl/env/Mamba3D/include -fPIC -O2 -isystem /media/jiang/jkl/env/Mamba3D/include -fPIC -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include/TH -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda/include -I/media/jiang/jkl/env/Mamba3D/include/python3.8 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=chamfer -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include/TH -I/media/jiang/jkl/env/Mamba3D/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda/include -I/media/jiang/jkl/env/Mamba3D/include/python3.8 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=chamfer -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /media/jiang/jkl/project/Mamba3D-main/extensions/chamfer_dist/build/temp.linux-x86_64-cpython-38/chamfer.o: cuda_compile /media/jiang/jkl/project/Mamba3D-main/extensions/chamfer_dist/chamfer.cu
build /media/jiang/jkl/project/Mamba3D-main/extensions/chamfer_dist/build/temp.linux-x86_64-cpython-38/chamfer_cuda.o: compile /media/jiang/jkl/project/Mamba3D-main/extensions/chamfer_dist/chamfer_cuda.cpp







